        "formidable": "^3.2.4",
        "github-markdown-css": "^5.1.0",
        "highlight.js": "^11.4.0",
        "json5": "^2.2.1",
        "markdown-it": "^12.3.2",
        "markdown-it-abbr": "^1.0.4",
        "markdown-it-anchor": "^8.4.1",
        "markdown-it-attrs": "^4.1.3",
        "memorystore": "^1.6.7",
        "minisearch": "^4.0.3",
        "node-html-parser": "^5.3.3",
        "prompts": "^2.4.2",
        "sass": "^1.52.3",
        "sitemap": "^7.1.1",
        "source-map": "^0.8.0-beta.0",
