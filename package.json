{
  "name": "beyond-easy",
  "version": "1.0.0",
  "description": "Node.js framework, with a lot of built in features",
  "main": "dist/index.js",
  "scripts": {
    "build": "node build",
    "writeGitLabCi": "node build/writeGitLabCi.js",
    "build:ts": "tsc --project tsconfig.json",
    "build:ts:production": "tsc --project production.tsconfig.json",
    "push": "node build/gitPush.js",
    "buildAndPush": "node build/buildProduction.js",
    "test:dist": "node --trace-warnings --inspect=127.0.0.1:9229 tests/core/index.js",
    "test": "npm run build & npm run test:dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/ido.very.much/beyond-easy.git"
  },
  "keywords": [
    "router",
    "backend",
    "http",
    "framework",
    "Razor",
    "Sqlite"
  ],
  "type": "module",
  "author": "Ido S.",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/ido.very.much/beyond-easy/issues"
  },
  "dependencies": {
